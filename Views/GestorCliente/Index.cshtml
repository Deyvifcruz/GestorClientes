@model List<GestorCliente.Models.ViewModels.GestorClienteVM>
@{
    ViewBag.Title = "Gestor de clientes";
}

<h2>@ViewBag.Title</h2>



<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>#</th>
                <th>Nombre</th>
                <th>Direccion 1</th>
                <th>Direccion 2</th>
                <th>Direccion 3</th>
            </tr>
            @foreach (var oElemento in Model)
            { 
                <tr>
                    <td>@oElemento.Id</td>
                    <td>@oElemento.Nombre</td>
                    <td>@oElemento.Direccion1</td>
                    <td>@oElemento.Direccion2</td>
                     <td>@oElemento.Direccion3</td>
                </tr>
            }
        </table>
    </div>
</div>